    controls                    : youtubeParamsDefault.controls,\\r\\n                    fs                          : youtubeParamsDefault.fs,\\r\\n                    autoplay                    : youtubeParamsDefault.autoplay,\\r\\n                    mute                        : youtubeParamsDefault.mute,\\r\\n                    showinfo                    : youtubeParamsDefault.showinfo,\\r\\n                    rel                         : youtubeParamsDefault.rel,\\r\\n                    start                       : youtubeParamsDefault.start,\\r\\n                    end                         : youtubeParamsDefault.end,\\r\\n                    loop                        : youtubeParamsDefault.loop,\\r\\n                    playlist                    : youtubeParamsDefault.playlist,\\r\\n                    list                        : youtubeParamsDefault.list,\\r\\n                    listtype                    : youtubeParamsDefault.listtype\\r\\n                }\\r\\n            }\\r\\n        },\\r\\n        computed  : {\\r\\n            instance() {\\r\\n                return CKEDITOR.instances[this.id]\\r\\n            },\\r\\n            layoutHorizontalAlignItems() {\\r\\n                return [\\r\\n                    {value: 'left', text: 'Left'},\\r\\n                    {value: 'center', text: 'Center'},\\r\\n                    {value: 'right', text: 'Right'}\\r\\n                ];\\r\\n            },\\r\\n            layoutVerticalAlignItems() {\\r\\n                return [\\r\\n                    {value: 'top', text: 'Top'},\\r\\n                    {value: 'middle', text: 'Middle'},\\r\\n                    {value: 'bottom', text: 'Bottom'}\\r\\n                ];\\r\\n            }\\r\\n        },\\r\\n        watch     : {\\r\\n            value(val) {\\r\\n                if (this.instance) {\\r\\n                    this.update(val)\\r\\n                }\\r\\n            }\\r\\n        },\\r\\n        mounted() {\\r\\n            this.create();\\r\\n        },\\r\\n        beforeDestroy() {\\r\\n            if (!this.destroyed) {\\r\\n                this.instance.focusManager.blur(true);\\r\\n                this.instance.removeAllListeners();\\r\\n                this.instance.destroy();\\r\\n                this.destroyed = true;\\r\\n                documentUtil.deleteDivWithClass('cke_float');\\r\\n                documentUtil.deleteDivWithClass('cke_dialog_background_cover');\\r\\n                documentUtil.deleteDivWithClass('cke_dialog');\\r\\n            }\\r\\n        },\\r\\n        methods   : {\\r\\n            //////// universal dialog methods\\r\\n            onUniversalDialogClosed(dialogName, model) {\\r\\n                this[dialogName + 'DialogVisible'] = false;\\r\\n                this.$emit(dialogName + '-dialog-closed', model);\\r\\n            },\\r\\n            onShowUniversalDialogRequested(dialogName, dialogData) {\\r\\n                this[dialogName + 'DialogVisible'] = true;\\r\\n\\r\\n                let empty = (Object.keys(dialogData).length === 0);\\r\\n\\r\\n                for(var propertyName in this[dialogName + 'Params']) {\\r\\n                    if (empty) {\\r\\n                        this[dialogName + 'Params'][propertyName] = [dialogName + 'ParamsDefault'][propertyName];\\r\\n                    } else {\\r\\n                        this[dialogName + 'Params'][propertyName] = dialogData[propertyName];\\r\\n                    }\\r\\n                }\\r\\n\\r\\n                this[dialogName + 'NewItem'] = empty;\\r\\n\\r\\n                return new Promise((resolve) => {\\r\\n                    this.$once(dialogName + '-dialog-closed', (data) => {\\r\\n                        resolve(data);\\r\\n                    });\\r\\n                })\\r\\n            },\\r\\n            ////////\\r\\n\\r\\n            //////// image dialog methods\\r\\n            onImageDialogClosed(image) {\\r\\n                this.galleryDialogVisible = false;\\r\\n                this.$emit('image-dialog-closed', image);\\r\\n            },\\r\\n            onShowImageDialogRequested(dialogData) {\\r\\n                if (!this.imagesListResource) {\\r\\n                    console.error('No images list resource set!');\\r\\n                    return;\\r\\n                }\\r\\n\\r\\n                let dialogName = 'gallery';\\r\\n                let empty = (Object.keys(dialogData).length === 0);\\r\\n\\r\\n                for(var propertyName in this[dialogName + 'Params']) {\\r\\n                    if (empty) {\\r\\n                        this[dialogName + 'Params'][propertyName] = galleryParamsDefault[propertyName];\\r\\n                    } else {\\r\\n                        this[dialogName + 'Params'][propertyName] = dialogData[propertyName];\\r\\n                    }\\r\\n                }\\r\\n\\r\\n                this.galleryNewItem = (Object.keys(dialogData).length === 1);\\r\\n\\r\\n                this.galleryDialogVisible = true;\\r\\n\\r\\n                let overlayDiv = document.querySelector('.cke_dialog_background_cover');\\r\\n                if (overlayDiv) {\\r\\n                    overlayDiv.style.display = 'none';\\r\\n                }\\r\\n                let ckDialogDiv = document.querySelector('.cke_dialog');\\r\\n                if (ckDialogDiv) {\\r\\n                    ckDialogDiv.style.display = 'none';\\r\\n                }\\r\\n                let toolbarDiv = document.querySelector('.cke_float');\\r\\n                if (toolbarDiv) {\\r\\n                    toolbarDiv.style.display = 'none';\\r\\n                }\\r\\n\\r\\n                return new Promise((resolve) => {\\r\\n                    this.$once('image-dialog-closed', (image) => {\\r\\n                        if (overlayDiv) {\\r\\n                            overlayDiv.style.display = 'block';\\r\\n                        }\\r\\n                        if (ckDialogDiv) {\\r\\n                            ckDialogDiv.style.display = 'block';\\r\\n                        }\\r\\n                        if (toolbarDiv) {\\r\\n                            toolbarDiv.style.display = 'block';\\r\\n                        }\\r\\n                        resolve(image);\\r\\n                    });\\r\\n                })\\r\\n            },\\r\\n            ////////\\r\\n\\r\\n            //////// grid dialog methods\\r\\n            onGridDialogClosed(model) {\\r\\n                this.gridDialogVisible = false;\\r\\n                this.$emit('grid-dialog-closed', model);\\r\\n            },\\r\\n            onShowGridItemPropsDialogRequested(props) {\\r\\n                this.gridItemPropsDialogVisible = true;\\r\\n                this.gridItemGrow               = props.grow;\\r\\n                return new Promise((resolve) => {\\r\\n                    this.$once('grid-item-props-dialog-closed', (data) => {\\r\\n                        resolve(data);\\r\\n                    });\\r\\n                })\\r\\n            },\\r\\n            onShowGridDialogRequested(gridData) {\\r\\n                this.gridDialogVisible = true;\\r\\n                this.gridOrientation   = gridData.direction;\\r\\n                this.gridNewItem = (Object.keys(gridData).length === 0);\\r\\n                return new Promise((resolve) => {\\r\\n                    this.$once('grid-dialog-closed', (data) => {\\r\\n                        resolve(data);\\r\\n                    });\\r\\n                })\\r\\n            },\\r\\n            onShowLayoutDialogRequested() {\\r\\n                this.layoutDialogVisible = true;\\r\\n                documentUtil.showDivWithClass('cke_dialog_background_cover', false);\\r\\n                documentUtil.showDivWithClass('cke_dialog', false);\\r\\n                documentUtil.showDivWithClass('cke_float', false);\\r\\n                return new Promise((resolve) => {\\r\\n                    this.$once('layout-dialog-closed', (data) => {\\r\\n                        resolve(data);\\r\\n                    });\\r\\n                })\\r\\n            },\\r\\n            onGridItemPropsDialogClosed(model) {\\r\\n                this.gridItemPropsDialogVisible = false;\\r\\n                this.$emit('grid-item-props-dialog-closed', model);\\r\\n            },\\r\\n            closePasteWarningDialog() {\\r\\n                this.showPasteWarningDialog = false;\\r\\n            },\\r\\n            onLayoutDialogClosed() {\\r\\n                documentUtil.showDivWithClass('cke_dialog_background_cover', true);\\r\\n                documentUtil.showDivWithClass('cke_dialog', true);\\r\\n                documentUtil.showDivWithClass('cke_float', true);\\r\\n                this.layoutDialogVisible = false;\\r\\n                this.$emit('layout-dialog-closed', null);\\r\\n            },\\r\\n            onLayoutSelected(layoutId) {\\r\\n                this.layoutDialogVisible = false;\\r\\n                this.$emit('layout-dialog-closed', {type: layoutId});\\r\\n            },\\r\\n            ////////\\r\\n\\r\\n            onPluginDialogCancelled(data) {\\r\\n                this.$set(this.pluginDialogsVisibility, data.name, false);\\r\\n                if (this.pluginDialogsCallbacks[data.name].onCancel) {\\r\\n                    this.pluginDialogsCallbacks[data.name].onCancel();\\r\\n                }\\r\\n            },\\r\\n            onPluginDialogSaved(data) {\\r\\n                this.$set(this.pluginDialogsVisibility, data.name, false);\\r\\n                if (this.pluginDialogsCallbacks[data.name].onSave) {\\r\\n                    this.pluginDialogsCallbacks[data.name].onSave(data.isNew, data.model);\\r\\n                }\\r\\n            },\\r\\n            showDialog(name, model) {\\r\\n                if (model) {\\r\\n                    this.$set(this.pluginDialogsModels, name, model);\\r\\n                } else {\\r\\n                    this.$set(this.pluginDialogsModels, name, {});\\r\\n                }\\r\\n                this.$set(this.pluginDialogsStates, name, {isNew: !!!model});\\r\\n                this.$set(this.pluginDialogsVisibility, name, true);\\r\\n            },\\r\\n            unregisterDialog(name) {\\r\\n                this.$delete(this.pluginDialogs, name);\\r\\n                this.$delete(this.pluginDialogsVisibility, name);\\r\\n                this.$delete(this.pluginDialogsStates, name);\\r\\n                delete this.pluginDialogsCallbacks[name];\\r\\n            },\\r\\n            registerDialog(component, name, onSaveCallback, onCancelCallback) {\\r\\n                this.$set(this.pluginDialogs, name, component);\\r\\n                this.$set(this.pluginDialogsVisibility, name, false);\\r\\n                this.$set(this.pluginDialogsStates, name, {isNew: true});\\r\\n                this.pluginDialogsCallbacks[name] = {\\r\\n                    onSave  : onSaveCallback,\\r\\n                    onCancel: onCancelCallback\\r\\n                }\\r\\n            },\\r\\n            runPluginRegistrators(editor, ckeditorConfig) {\\r\\n                this.plugins.map((plugin) => {\\r\\n                    plugin.init(this, editor, ckeditorConfig);\\r\\n                    plugin.register(editor, ckeditorConfig);\\r\\n                })\\r\\n            },\\r\\n            initializePluginButtons(editor, ckeditorConfig) {\\r\\n                log(`Editor ${this.id} got ${this.plugins.length} plugins`);\\r\\n                this.plugins.map((plugin) => {\\r\\n                    let button = plugin.getButton();\\r\\n                    if (!button) {\\r\\n                        return;\\r\\n                    }\\r\\n                    log(`Plugin ${plugin} got button`);\\r\\n                    if (button.onClicked) {\\r\\n                        editor.addCommand(plugin.name + 'Command', {\\r\\n                            exec: button.onClicked\\r\\n                        });\\r\\n                    } else {\\r\\n                        console.warn(`Plugin ${plugin.name} registered a button but does not define click handler on it (button.onClicked)`);\\r\\n                    }\\r\\n                    editor.ui.addButton(plugin.name + 'Button', button);\\r\\n                })\\r\\n            },\\r\\n            registerPluginWidgets(editor, ckeditorConfig) {\\r\\n                let self = this;\\r\\n                this.plugins.map((plugin) => {\\r\\n                    let widget = plugin.getEditorWidget();\\r\\n                    if (!widget) {\\r\\n                        return;\\r\\n                    }\\r\\n                    editor.widgets.add(plugin.name, {\\r\\n                        allowedContent : true,\\r\\n                        init           : function() {\\r\\n                            let widget = this;\\r\\n                            if (widget.element.$.dataset.data) {\\r\\n                                try {\\r\\n                                    let data = JSON.parse(widget.element.$.dataset.data);\\r\\n                                    widget.setData('data', data);\\r\\n                                } catch (e) {\\r\\n                                    console.error('Unable to widget data', e);\\r\\n                                }\\r\\n                            }\\r\\n                        },\\r\\n                        upcast         : function(element) {\\r\\n                            return (element.name === 'div' && element.hasClass(plugin.name)) || element.name === plugin.name;\\r\\n                        },\\r\\n                        edit           : function() {\\r\\n                            return widget.onEditRequested(this);\\r\\n                        },\\r\\n                        template       : '<div class=\\\"' + plugin.name + '\\\"></div>',\\r\\n                        requiredContent: 'div(' + plugin.name + ')',\\r\\n                        data           : function() {\\r\\n                            widget.onChange(this);\\r\\n                        },\\r\\n                    })\\r\\n                })\\r\\n            },\\r\\n            removeElement(tagName, element) {\\r\\n                let subElements = element.querySelectorAll(tagName);\\r\\n                for (let i = 0; i < subElements.length; i++) {\\r\\n                    subElements[i].parentNode.removeChild(subElements[i]);\\r\\n                }\\r\\n                return subElements.length;\\r\\n            },\\r\\n            initCknativePlugins() {\\r\\n                let comparsionPlugin                    = ComparsionPlugin.createAndRegister(CKEDITOR);\\r\\n                comparsionPlugin.onShowDialogRequested  = this.onShowImageDialogRequested;\\r\\n                comparsionPlugin.confirm                = this.confirm;\\r\\n\\r\\n                let facebookPlugin                      = FacebookPlugin.createAndRegister(CKEDITOR);\\r\\n                facebookPlugin.onShowDialogRequested    = this.onShowUniversalDialogRequested;\\r\\n                facebookPlugin.confirm                  = this.confirm;\\r\\n\\r\\n                let galleryPlugin                       = GalleryPlugin.createAndRegister(CKEDITOR);\\r\\n                galleryPlugin.onShowDialogRequested     = this.onShowImageDialogRequested;\\r\\n                galleryPlugin.confirm                   = this.confirm;\\r\\n\\r\\n                let giphyPlugin                         = GiphyPlugin.createAndRegister(CKEDITOR);\\r\\n                giphyPlugin.onShowDialogRequested       = this.onShowUniversalDialogRequested;\\r\\n                giphyPlugin.confirm                     = this.confirm;\\r\\n\\r\\n                let imgurPlugin                         = ImgurPlugin.createAndRegister(CKEDITOR);\\r\\n                imgurPlugin.onShowDialogRequested       = this.onShowUniversalDialogRequested;\\r\\n                imgurPlugin.confirm                     = this.confirm;\\r\\n\\r\\n                let instagramPlugin                     = InstagramPlugin.createAndRegister(CKEDITOR);\\r\\n                instagramPlugin.onShowDialogRequested   = this.onShowUniversalDialogRequested;\\r\\n                instagramPlugin.confirm                 = this.confirm;\\r\\n\\r\\n                let mixerPlugin                         = MixerPlugin.createAndRegister(CKEDITOR);\\r\\n                mixerPlugin.onShowDialogRequested       = this.onShowUniversalDialogRequested;\\r\\n                mixerPlugin.confirm                     = this.confirm;\\r\\n\\r\\n                let picturePlugin                       = PicturePlugin.createAndRegister(CKEDITOR);\\r\\n                picturePlugin.onShowDialogRequested     = this.onShowImageDialogRequested;\\r\\n                picturePlugin.confirm                   = this.confirm;\\r\\n\\r\\n                let soundcloudPlugin                    = SoundcloudPlugin.createAndRegister(CKEDITOR);\\r\\n                soundcloudPlugin.onShowDialogRequested  = this.onShowUniversalDialogRequested;\\r\\n                soundcloudPlugin.confirm                = this.confirm;\\r\\n\\r\\n                let tweetPlugin                         = TweetPlugin.createAndRegister(CKEDITOR);\\r\\n                tweetPlugin.onShowDialogRequested       = this.onShowUniversalDialogRequested;\\r\\n                tweetPlugin.confirm                     = this.confirm;\\r\\n\\r\\n                let twitchPlugin                        = TwitchPlugin.createAndRegister(CKEDITOR);\\r\\n                twitchPlugin.onShowDialogRequested      = this.onShowUniversalDialogRequested;\\r\\n                twitchPlugin.confirm                    = this.confirm;\\r\\n\\r\\n                let vimeoPlugin                         = VimeoPlugin.createAndRegister(CKEDITOR);\\r\\n                vimeoPlugin.onShowDialogRequested       = this.onShowUniversalDialogRequested;\\r\\n                vimeoPlugin.confirm                     = this.confirm;\\r\\n\\r\\n                let youtubePlugin                       = YoutubePlugin.createAndRegister(CKEDITOR);\\r\\n                youtubePlugin.onShowDialogRequested     = this.onShowUniversalDialogRequested;\\r\\n                youtubePlugin.confirm                   = this.confirm;\\r\\n\\r\\n                BackToContentPlugin.createAndRegister(CKEDITOR);\\r\\n\\r\\n                let authorPlugin = AuthorPlugin.createAndRegister(CKEDITOR);\\r\\n                authorPlugin.setListResource(this.authorsListResource);\\r\\n\\r\\n                let gridPugin                           = GridPlugin.createAndRegister(CKEDITOR);\\r\\n                gridPugin.onShowDialogRequested         = this.onShowGridDialogRequested;\\r\\n                gridPugin.onShowLayoutDialogRequested   = this.onShowLayoutDialogRequested;\\r\\n                gridPugin.onShowGridItemDialogRequested = this.onShowGridItemPropsDialogRequested;\\r\\n                gridPugin.confirm                       = this.confirm;\\r\\n            },\\r\\n            initPasteFilter(editor, ckeditorConfig) {\\r\\n                if (this.clearStylesOnPaste) {\\r\\n                    log(`ckEditor instance ${this.id}: Paste filter active.`);\\r\\n                    let self = this;\\r\\n                    editor.on('paste', function(evt) {\\r\\n                        if (evt.data.dataValue === '' && evt.data.method === 'drop') {\\r\\n                            return;\\r\\n                        }\\r\\n                        try {\\r\\n                            let element           = document.createElement('div');\\r\\n                            element.style.display = 'none';\\r\\n                            element.innerHTML     = evt.data.dataValue;\\r\\n                            document.body.appendChild(element);\\r\\n                            let subElements = element.querySelectorAll('*');\\r\\n                            for (let i = 0; i < subElements.length; i++) {\\r\\n                                subElements[i].removeAttribute('style');\\r\\n                                subElements[i].removeAttribute('lang');\\r\\n                                subElements[i].removeAttribute('class');\\r\\n                                subElements[i].removeAttribute('id');\\r\\n                                subElements[i].removeAttribute('onclick');\\r\\n                                Object.keys(subElements[i].dataset).map((keyName) => {\\r\\n                                    delete subElements[i].dataset[keyName];\\r\\n                                })\\r\\n                            }\\r\\n\\r\\n                            //clear all the spans\\r\\n                            let spans = element.querySelectorAll('span');\\r\\n                            for (let i = 0; i < spans.length; i++) {\\r\\n                                try {\\r\\n                                    documentUtil.unwrapElement(spans[i]);\\r\\n                                } catch (e) {\\r\\n                                    //ignore\\r\\n                                }\\r\\n                            }\\r\\n\\r\\n                            let showWarning = false;\\r\\n\\r\\n                            let matchesCount = self.removeElement('iframe', element);\\r\\n                            if (matchesCount > 0) {\\r\\n                                showWarning = true;\\r\\n                            }\\r\\n                            matchesCount = self.removeElement('style', element);\\r\\n                            if (matchesCount > 0) {\\r\\n                                showWarning = true;\\r\\n                            }\\r\\n                            matchesCount = self.removeElement('img', element);\\r\\n                            if (matchesCount > 0) {\\r\\n                                showWarning = true;\\r\\n                            }\\r\\n                            matchesCount = self.removeElement('script', element);\\r\\n                            if (matchesCount > 0) {\\r\\n                                showWarning = true;\\r\\n                            }\\r\\n                            if (showWarning) {\\r\\n                                self.showPasteWarningDialog = true;\\r\\n                            }\\r\\n                            evt.data.dataValue = element.innerHTML;\\r\\n                        } catch (e) {\\r\\n                            console.error('Unable to paste content', e);\\r\\n                        }\\r\\n                    })\\r\\n                }\\r\\n            },\\r\\n            replaceAll(value, search, replacement) {\\r\\n                return value.replace(new RegExp(search, 'g'), replacement);\\r\\n            },\\r\\n            async create() {\\r\\n                const defaults = {\\r\\n                    allowedContent: true,\\r\\n                    extraPlugins  : 'widget,widgetselection,colorbutton,colordialog,justify,font',\\r\\n                };\\r\\n\\r\\n                let configuration = Object.assign({}, defaults, this.editorOptions);\\r\\n                if (this.toolbar) {\\r\\n                    configuration.toolbar = this.toolbar;\\r\\n                }\\r\\n                if (this.height) {\\r\\n                    configuration.height = this.height;\\r\\n                }\\r\\n\\r\\n                if (typeof CKEDITOR === 'undefined') {\\r\\n                    log('ckEditor not loaded. Loading from CDN.');\\r\\n                    await loader.ensureLoaded();\\r\\n                }\\r\\n                this.initCknativePlugins();\\r\\n\\r\\n                this.pluginsCustom.forEach(element => {\\r\\n                    configuration.extraPlugins += ',' + element;\\r\\n                });\\r\\n\\r\\n                let inToolbar = false;\\r\\n                configuration.toolbar.forEach(element => {\\r\\n                    let i          = 0;\\r\\n                    let rowFitting = true;\\r\\n                    element.forEach(str => {\\r\\n                        if (str !== this.pluginsCustom[i++]) rowFitting = false;\\r\\n                    });\\r\\n                    inToolbar = rowFitting ? true : inToolbar;\\r\\n                });\\r\\n                if (!inToolbar) {\\r\\n                    configuration.toolbar.push(this.pluginsCustom);\\r\\n                }\\r\\n\\r\\n                CKEDITOR.disableAutoInline = true;\\r\\n                CKEDITOR.inline(this.id, configuration);\\r\\n\\r\\n                let plugins = [\\r\\n                    'app-author',\\r\\n                    'app-back-to-contents',\\r\\n                    'app-comparsion',\\r\\n                    'app-facebook',\\r\\n                    'app-gallery',\\r\\n                    'app-gallery-slide',\\r\\n                    'app-giphy',\\r\\n                    'app-imgur',\\r\\n                    'app-instagram',\\r\\n                    'app-mixer',\\r\\n                    'app-picture',\\r\\n                    'app-soundcloud',\\r\\n                    'app-tweet',\\r\\n                    'app-twitch',\\r\\n                    'app-vimeo',\\r\\n                    'app-youtube'\\r\\n                ];\\r\\n\\r\\n                for (let i = 0; i < plugins.length; i++) {\\r\\n                    CKEDITOR.dtd[plugins[i]] = {'#': 1};\\r\\n                    for (let tagName in CKEDITOR.dtd) {\\r\\n                        if (CKEDITOR.dtd[tagName].div) {\\r\\n                            CKEDITOR.dtd[tagName][plugins[i]] = 1;\\r\\n                        }\\r\\n                    }\\r\\n                }\\r\\n\\r\\n                CKEDITOR.dtd['app-gallery']['app-gallery-slide']    = {'#': 1};\\r\\n                CKEDITOR.dtd['app-gallery-slide']['app-picture']    = {'#': 1};\\r\\n                CKEDITOR.dtd['app-comparsion']['app-gallery-slide'] = {'#': 1};\\r\\n\\r\\n                //this.instance.setData(this.value)\\r\\n                this.instance.on('change', this.onChange);\\r\\n                this.instance.on('blur', this.onBlur);\\r\\n                this.instance.on('focus', this.onFocus);\\r\\n                this.initializePluginButtons(this.instance, configuration);\\r\\n                this.initPasteFilter(this.instance, configuration);\\r\\n                CKEDITOR.on('instanceReady', (evt) => {\\r\\n                    if (evt.editor.name !== this.id) {\\r\\n                        return;\\r\\n                    }\\r\\n                    log(`ckEditor instance ${this.id} ready`);\\r\\n                    evt.editor.dataProcessor.htmlFilter.addRules({\\r\\n                        elements: {\\r\\n                            a: function(element) {\\r\\n                                element.attributes.rel = 'noopener noreferrer';\\r\\n                            }\\r\\n                        }\\r\\n                    });\\r\\n                    evt.editor.removeMenuItem('paste');\\r\\n                    if (this.plugins) {\\r\\n                        this.runPluginRegistrators(evt.editor, configuration);\\r\\n                        this.registerPluginWidgets(evt.editor, configuration);\\r\\n                    }\\r\\n                    //hack to init custom widgets\\r\\n                    setTimeout(() => {\\r\\n                        this.instance.setData(this.replaceAll(this.value, '<p>&nbsp;</p>', ''));\\r\\n                    }, 300);\\r\\n                });\\r\\n            },\\r\\n            update(val) { //used by component when value changes from outside the editor\\r\\n                let html = this.instance.getData();\\r\\n                if (html !== val) {\\r\\n                    this.instance.setData(this.replaceAll(val, '<p>&nbsp;</p>', ''));\\r\\n                }\\r\\n            },\\r\\n\\r\\n            onChange() { //used from editor hooks\\r\\n                if (this.changeTimer) {\\r\\n                    clearTimeout(this.changeTimer);\\r\\n                }\\r\