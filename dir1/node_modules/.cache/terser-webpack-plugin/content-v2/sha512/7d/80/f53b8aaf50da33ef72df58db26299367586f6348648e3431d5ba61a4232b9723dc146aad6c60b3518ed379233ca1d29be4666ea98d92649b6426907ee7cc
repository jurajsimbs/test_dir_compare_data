    };\\r\\n    const giphyParamsDefault = {\\r\\n        id                  : null,\\r\\n        responsive          : true,\\r\\n        height              : '300',\\r\\n        width               : '300',\\r\\n        url                 : null\\r\\n    };\\r\\n    const imgurParamsDefault = {\\r\\n        id                  : null,\\r\\n        responsive          : true,\\r\\n        height              : '400',\\r\\n        width               : '400'\\r\\n    };\\r\\n    const instagramParamsDefault = {\\r\\n        id                  : null,\\r\\n        responsive          : true,\\r\\n        height              : '400',\\r\\n        width               : '400'\\r\\n    };\\r\\n    const mixerParamsDefault = {\\r\\n        id                  : null,\\r\\n        type                : 'player',\\r\\n        hidechannel         : false,\\r\\n        muted               : false,\\r\\n        disablecostream     : false,\\r\\n        disablelowlatency   : false\\r\\n    };\\r\\n    const soundcloudParamsDefault = {\\r\\n        url                 : null,\\r\\n        autoplay            : false,\\r\\n        artwork             : false,\\r\\n        color               : null\\r\\n    };\\r\\n    const tweetParamsDefault = {\\r\\n        id                  : null,\\r\\n        showcard            : true,\\r\\n        darktheme           : false\\r\\n    };\\r\\n    const twitchParamsDefault = {\\r\\n        src                 : null,\\r\\n        srctype             : null,\\r\\n        autoplay            : false,\\r\\n        muted               : false,\\r\\n        time                : null\\r\\n    };\\r\\n    const vimeoParamsDefault = {\\r\\n        id                  : null,\\r\\n        loop                : false,\\r\\n        title               : false,\\r\\n        fullscreen          : true\\r\\n    };\\r\\n    const youtubeParamsDefault = {\\r\\n        videourl            : null,\\r\\n        controls            : true,\\r\\n        