true;\\r\\n                        this.youtubeLoop = false;\\r\\n                        this.youtubeListtype = null;\\r\\n                        this.youtubeList = null;\\r\\n                        this.youtubePlaylist = null;\\r\\n                        break;\\r\\n                    case 'loop':\\r\\n                        this.youtubeLoop = true;\\r\\n                        this.youtubeRel = false;\\r\\n                        this.youtubeListtype = null;\\r\\n                        this.youtubeList = null;\\r\\n                        this.youtubePlaylist = null;\\r\\n                        break;\\r\\n                    case 'playlist':\\r\\n                        this.youtubeLoop = false;\\r\\n                        this.youtubeRel = false;\\r\\n                        this.youtubeListtype = null;\\r\\n                        this.youtubeList = null;\\r\\n                        this.youtubePlaylist = null;\\r\\n                        break;\\r\\n                    case 'playlistID':\\r\\n                    case 'search':\\r\\n                    case 'user_uploads':\\r\\n                    default:\\r\\n                        this.youtubeListtype = this.endtype;\\r\\n                        this.youtubeLoop = false;\\r\\n                        this.youtubeRel = false;\\r\\n                        this.youtubeList = null;\\r\\n                        this.youtubePlaylist = null;\\r\\n                }\\r\\n            }\\r\\n        },\\r\\n        props: {\\r\\n            visible     : {\\r\\n                type    : Boolean,\\r\\n                default : false\\r\\n            },\\r\\n            newitem     : {\\r\\n                type    : Boolean,\\r\\n                default : false\\r\\n            },\\r\\n            videourl    : {\\r\\n                type    : String,\\r\\n                default : null\\r\\n            },\\r\\n            controls    : {\\r\\n                type    : Boolean,\\r\\n                default : t