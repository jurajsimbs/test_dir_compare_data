               url                         : facebookParamsDefault.url,\\r\\n                    type                        : facebookParamsDefault.type,\\r\\n                    tabs                        : facebookParamsDefault.tabs,\\r\\n                    height                      : facebookParamsDefault.height,\\r\\n                    width                       : facebookParamsDefault.width,\\r\\n                    responsive                  : facebookParamsDefault.responsive\\r\\n                },\\r\\n\\r\\n                galleryDialogVisible        : false,\\r\\n                galleryNewItem              : true,\\r\\n                galleryParams               : {\\r\\n                    title                       : galleryParamsDefault.title,\\r\\n                    description                 : galleryParamsDefault.description,\\r\\n                    padding                     : galleryParamsDefault.padding,\\r\\n                    carousel                    : galleryParamsDefault.carousel,\\r\\n                    images                      : galleryParamsDefault.images,\\r\\n                    type                        : galleryParamsDefault.type,\\r\\n                },\\r\\n\\r\\n                giphyDialogVisible          : false,\\r\\n                giphyNewItem                : true,\\r\\n                giphyParams                 : {\\r\\n                    id                          : giphyParamsDefault.id,\\r\\n                    responsive                  : giphyParamsDefault.responsive,\\r\\n                    height                      : giphyParamsDefault.height,\\r\\n                    width                       : giphyParamsDefault.width,\\r\\n                    url                         : giphyParamsDefault.url\\r\\n                },\\r\\n\\r\\n                imgurDialogVisible          : false,\\r\\n                imgurNewItem                : true,\\r\\n                imgurParams                 : {\\r\\n                    id                          : imgurParamsDefault.id,\\r\\n                    responsive                  : imgurParamsDefault.responsive,\\r\\n                    height                      : imgurParamsDefault.height,\\r\\n                    width                       : imgurParamsDefault.width\\r\\n                },\\r\\n\\r\\n                instagramDialogVisible      : false,\\r\\n                instagramNewItem            : true,\\r\\n                instagramParams             : {\\r\\n                    id                          : instagramParamsDefault.id,\\r\\n                    responsive                  : instagramParamsDefault.responsive,\\r\\n                    height                      : instagramParamsDefault.height,\\r\\n                    width                       : instagramParamsDefault.width\\r\\n                },\\r\\n\\r\\n                mixerDialogVisible          : false,\\r\\n                mixerNewItem                : true,\\r\\n                mixerParams                 : {\\r\\n                    id                          : mixerParamsDefault.id,\\r\\n                    type                        : mixerParamsDefault.type,\\r\\n                    hidechannel                 : mixerParamsDefault.hidechannel,\\r\\n                    muted                       : mixerParamsDefault.muted,\\r\\n                    disablecostream             : mixerParamsDefault.disablecostream,\\r\\n                    disablelowlatency           : mixerParamsDefault.disablelowlatency\\r\\n                },\\r\\n\\r\\n                soundcloudDialogVisible     : false,\\r\\n                soundcloudNewItem           : true,\\r\\n                soundcloudParams            : {\\r\\n                    url                         : soundcloudParamsDefault.url,\\r\\n                    autoplay                    : soundcloudParamsDefault.autoplay,\\r\\n                    artwork                     