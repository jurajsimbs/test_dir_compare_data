\\r\\n            alt         : {\\r\\n                type    : String,\\r\\n                default : ''\\r\\n            }\\r\\n        },\\r\\n        data() {\\r\\n            return {\\r\\n                dialogVisible   : this.visible,\\r\\n                imageTitle      : this.title,\\r\\n                imageDescription: this.description,\\r\\n                imageAlt        : this.alt\\r\\n            }\\r\\n        },\\r\\n        watch: {\\r\\n            visible() {\\r\\n                this.dialogVisible = this.visible;\\r\\n            },\\r\\n            dialogVisible() {\\r\\n                if (this.dialogVisible) {\\r\\n                    this.imageTitle         = this.title;\\r\\n                    this.imageDescription   = this.description;\\r\\n                    this.imageAlt           = this.alt;\\r\\n                } else {\\r\\n                    this.closeDialog();\\r\\n                }\\r\\n            },\\r\\n        },\\r\\n        methods : {\\r\\n            closeDialog() {\\r\\n                this.$emit('close', null);\\r\\n            },\\r\\n            done() {\\r\\n                this.$emit('close', {\\r\\n                    title       : this.imageTitle,\\r\\n                    description : this.imageDescription,\\r\\n                    alt         : this.imageAlt,\\r\\n                    code        : this.code\\r\\n                });\\r\\n            }\\r\\n        }\\r\\n    }\\r\\n</script>\\r\\n<style scoped lang=\\\"scss\\\">\\r\\n    .preview {\\r\\n        max-height: 200px;\\r\\n    }\\r\\n</style>\",\"import mod from \\\"-!../../../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../../../node_modules/thread-loader/dist/cjs.js!../../../../node_modules/babel-loader/lib/index.js!../../../../node_modules/cache-loader/dist/cjs.js??ref--0-0!../../../../node_modules/vue-loader/lib/index.js??vue-loader-options!./imagePropertiesDialog.vue?vue&type=script&lang=js&\\\"; export default mod; export * from \\\"