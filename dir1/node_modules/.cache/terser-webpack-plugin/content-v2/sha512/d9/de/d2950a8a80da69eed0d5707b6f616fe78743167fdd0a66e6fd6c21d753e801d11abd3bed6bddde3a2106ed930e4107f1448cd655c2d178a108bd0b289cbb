lf.confirm('Picture will be lost! Delete?');\\r\\n                if (confirmation) {\\r\\n                    while (picture.firstChild) {\\r\\n                        picture.removeChild(picture.firstChild);\\r\\n                    }\\r\\n                    picture.parentElement.removeChild(picture);\\r\\n                }\\r\\n                editor.fire('change');\\r\\n            }\\r\\n        });\\r\\n        editor.addCommand(this.code, {\\r\\n            exec: async function () {\\r\\n                let selection = editor.getSelection();\\r\\n                let element = selection.getStartElement();\\r\\n\\r\\n                if (element) {\\r\\n                    element = element.getAscendant(self.ckOptions.element, true);\\r\\n                }\\r\\n\\r\\n                if (!element || element.getName() !== self.ckOptions.element || element) {\\r\\n                    element = editor.document.createElement('div');\\r\\n                    element.$.classList.add('picture');\\r\\n\\r\\n                    let newProps = await self.onShowDialogRequested({ type: 'picture' });\\r\\n                    if (!newProps) {\\r\\n                        return;\\r\\n                    }\\r\\n\\r\\n                    let item = editor.document.createElement('app-picture');\\r\\n                    item.setAttribute('code', newProps.images[0].code);\\r\\n                    item.setAttribute('thumb-sizes', newProps.images[0].thumbSizes);\\r\\n                    item.setAttribute('thumb-formats', newProps.images[0].thumbFormats);\\r\\n                    item.setAttribute('title', newProps.images[0].title);\\r\\n                    item.setAttribute('description', newProps.images[0].description);\\r\\n                    item.setAttribute('alt', newProps.images[0].alt);\\r\\n                    item.setAttribute('lightbox', newProps.images[0].lightbox);\\r\\n\\r\\n                    let date = new Date();\\r\\n                    let id = 'created_at_' + da