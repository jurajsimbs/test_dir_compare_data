                  :rules=\"[() => (imgurId && (imgurId.length > 0)) || 'Required field']\"\r\n                    ></v-text-field>\r\n                    <v-switch\r\n                        label=\"Responsive\"\r\n                        v-model=\"imgurResponsive\"\r\n                    ></v-switch>\r\n                    <v-text-field\r\n                        :label=\"imgurHeight ? 'Height' : ''\"\r\n                        placeholder=\"Height\"\r\n                        v-show=\"!imgurResponsive\"\r\n                        v-model=\"imgurHeight\"\r\n                        :rules=\"[() => (!(imgurHeight && (imgurHeight.length > 0)) || (Number.isInteger(Number(imgurHeight)))) || 'Must be a whole number']\"\r\n                    ></v-text-field>\r\n                    <v-text-field\r\n                        :label=\"imgurWidth ? 'Width' : ''\"\r\n                        placeholder=\"Width\"\r\n                        v-show=\"!imgurResponsive\"\r\n                        v-model=\"imgurWidth\"\r\n                        :rules=\"[() => (!(imgurWidth && (imgurWidth.length > 0)) || (Number.isInteger(Number(imgurWidth)))) || 'Must be a whole number']\"\r\n                    ></v-text-field>\r\n                </v-card-text>\r\n                <v-card-actions>\r\n                    <v-spacer></v-spacer>\r\n                    <v-btn color=\"blue darken-1\" flat @click.native=\"onClosed\">Close</v-btn>\r\n                    <v-btn color=\"blue darken-1\" flat @click.native=\"onSaved\" v-if=\"valid\">{{ newitem ? 'Add' : 'Save' }}</v-btn>\r\n                    <v-btn color=\"blue darken-1\" flat @click.native=\"onSaved\" v-else disabled>{{ newitem ? 'Add' : 'Save' }}</v-btn>\r\n                </v-card-actions>\r\n            </v-card>\r\n        </v-dialog>\r\n    </div>\r\n</template>\r\n<script>\r\n    import documentUtil from './../../../../lib/util/document';\r\n\r\n    export default {\r\n        data() {\r\n            return {\r\n                dialogVisible   : this.visible,\r\n                imgurId         : this.id,\r\n                imgurResponsive : this.responsive,\r\n                imgurHeight     : this.height,\r\n                imgurWidth      : this.width\r\n            }\r\n        },\r\n        computed: {\r\n            valid() {\r\n                return this.imgurId && (this.imgurId.length > 0) &&\r\n                    (!(this.imgurHeight && (this.imgurHeight.length > 0)) || (Number.isInteger(Number(this.imgurHeight))) || this.imgurResponsive) &&\r\n                    (!(this.imgurWidth && (this.imgurWidth.length > 0)) || (Number.isInteger(Number(this.imgurWidth))) || this.imgurResponsive);\r\n            }\r\n        },\r\n        methods: {\r\n            onClosed() {\r\n                this.$emit('close', 'imgur', null);\r\n            },\r\n            onSaved() {\r\n                this.$emit('close', 'imgur', {\r\n                    id          : this.imgurId,\r\n                    responsive  : this.imgurResponsive,\r\n                    height      : this.imgurHeight,\r\n                    width       : this.imgurWidth\r\n                });\r\n            }\r\n        },\r\n        watch: {\r\n            visible() {\r\n                this.dialogVisible = this.visible;\r\n            },\r\n            dialogVisible() {\r\n                if (this.dialogVisible) {\r\n                    this.imgurId            = this.id;\r\n                    this.imgurResponsive    = this.responsive;\r\n                    this.imgurHeight        = this.height;\r\n                    this.imgurWidth         = this.width;\r\n                } else {\r\n                    this.onClosed();\r\n                }\r\n            },\r\n            imgurResponsive() {\r\n                if (!this.imgurResponsive) {\r\n                    if (!Number.isInteger(Number(this.imgurHeight))) {\r\n                        this.imgurHeight = '870';\r\n                    }\r\n                    if (!Number.isInteger(Number(this.imgurWidth))) {\r\n                        this.imgurWidth = '740';\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        props: {\r\n            visible     : {\r\n                type    : Boolean,\r\n                default : false\r\n            },\r\n            newitem     : {\r\n                type    : Boolean,\r\n                default : false\r\n            },\r\n            id          : {\r\n                type    : String,\r\n                default : null\r\n            },\r\n            responsive  : {\r\n                type    : Boolean,\r\n                default : true\r\n            },\r\n            height      : {\r\n                type    : String,\r\n                default : '870'\r\n            },\r\n            width       : {\r\n                type    : String,\r\n                default : '740'\r\n            }\r\n        }\r\n    }\r\n</script>\r\n"],"sourceRoot":"src/components/editor/widgets/imgur"}]}