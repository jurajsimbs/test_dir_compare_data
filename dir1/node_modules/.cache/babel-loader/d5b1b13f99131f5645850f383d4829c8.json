ment';\r\n\r\n    const pattern = /(https?:\\/\\/(?:www\\.|(?!www))[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|www\\.[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|https?:\\/\\/(?:www\\.|(?!www))[a-zA-Z0-9]\\.[^\\s]{2,}|www\\.[a-zA-Z0-9]\\.[^\\s]{2,})/i;\r\n\r\n    export default {\r\n        data() {\r\n            return {\r\n                dialogVisible       : this.visible,\r\n                facebookUrl         : this.url,\r\n                facebookType        : this.type,\r\n                facebookTabs        : this.tabs,\r\n                facebookResponsive  : this.responsive,\r\n                facebookWidth       : this.width,\r\n                facebookHeight      : this.height,\r\n                pattern\r\n            }\r\n        },\r\n        computed: {\r\n            valid() {\r\n                return (this.facebookUrl && (this.facebookUrl.length > 0) && (this.facebookUrl.match(pattern))) &&\r\n                    (!(this.facebookWidth && (this.facebookWidth.length > 0)) || (Number.isInteger(Number(this.facebookWidth))) || this.facebookResponsive) &&\r\n                    (!(this.facebookHeight && (this.facebookHeight.length > 0)) || (Number.isInteger(Number(this.facebookHeight))) || this.facebookResponsive);\r\n            }\r\n        },\r\n        methods: {\r\n            onClosed() {\r\n                this.$emit('close', 'facebook', null);\r\n            },\r\n            onSaved() {\r\n                this.$emit('close', 'facebook', {\r\n                    url         : this.facebookUrl,\r\n                    type        : this.facebookType,\r\n                    tabs        : this.facebookTabs,\r\n                    responsive  : this.facebookResponsive,\r\n                    width       : (this.facebookWidth && (this.facebookWidth != 'null') ? this.facebookWidth : ''),\r\n                    height      : (this.facebookHeight && (this.facebookHeight != 'null') ? this.facebookHeight : '')\r\n                });\r\n          