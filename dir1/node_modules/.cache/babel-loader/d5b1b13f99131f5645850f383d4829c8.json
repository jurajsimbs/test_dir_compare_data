             </v-card-title>\r\n                <v-card-text>\r\n                    <v-text-field\r\n                        :label=\"facebookUrl ? 'URL (post, image, video)' : ''\"\r\n                        placeholder=\"URL (post, image, video)\"\r\n                        v-model=\"facebookUrl\"\r\n                        required\r\n                        :rules=\"[() => \r\n                            (\r\n                                facebookUrl && \r\n                                (facebookUrl.length > 0) && \r\n                                (facebookUrl.match(pattern) != null)\r\n                            ) || 'Required field, needs to be correct url format']\"\r\n                    ></v-text-field>\r\n\r\n                    Type\r\n                    <v-radio-group v-model=\"facebookType\">\r\n                        <v-radio\r\n                            label=\"Post\"\r\n                            value=\"post\"\r\n                        ></v-radio>\r\n                        <v-radio\r\n                            label=\"Video\"\r\n                            value=\"video\"\r\n                        ></v-radio>\r\n                        <v-radio\r\n                            label=\"Page\"\r\n                            value=\"page\"\r\n                        ></v-radio>\r\n                    </v-radio-group>\r\n\r\n                    <div v-if=\"facebookType == 'page'\">\r\n                        Page display type\r\n                        <v-radio-group v-model=\"facebookTabs\">\r\n                            <v-radio\r\n                                label=\"Timeline\"\r\n                                value=\"timeline\"\r\n                            ></v-radio>\r\n                            <v-radio\r\n                                label=\"Messages\"\r\n                                value=\"messages\"\r\n                            ></v-radio>\r\n                            <v-radio\r\n                            