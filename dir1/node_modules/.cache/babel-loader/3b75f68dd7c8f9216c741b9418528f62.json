{title}}</span>\r\n            <!--\r\n            <v-spacer/>\r\n            <v-btn v-if=\"preview\" @click=\"deleteFromServer\" color=\"error\">\r\n                DELETE FROM SERVER <v-icon>delete</v-icon>\r\n            </v-btn>\r\n            -->\r\n        </v-card-title>\r\n        <v-divider/>\r\n        <template v-if=\"asset\">\r\n            <v-card-media contain :src=\"asset\" :height=\"height + 'px'\">\r\n                <v-btn icon :href=\"asset\" target=\"_blank\">\r\n                    <v-icon>open_in_new</v-icon>\r\n                </v-btn>\r\n            </v-card-media>\r\n            <v-divider/>\r\n        </template>\r\n        <template v-if=\"preview\">\r\n            <v-card-media :height=\"height + 'px'\" contain :src=\"preview\" @click=\"showOriginal\" class=\"preview\" />\r\n            <v-divider/>\r\n        </template>\r\n        <template v-if=\"$slots.content\">\r\n            <v-card-text>\r\n                <slot name=\"content\"/>\r\n            </v-card-text>\r\n            <v-divider/>\r\n        </template>\r\n        <v-card-actions>\r\n            <FilePicker\r\n                    label=\"Dialog Background\"\r\n                    accept=\"image/*\"\r\n                    :assetType=\"assetType\"\r\n                    @pick=\"setPreview\"\r\n                    @remove=\"removeFile\"\r\n                    v-model=\"file\"\r\n            />\r\n        </v-card-actions>\r\n    </v-card>\r\n</template>\r\n\r\n<script>\r\n\r\n    const log = require('debug')('launcher:asset');\r\n\r\n    import FilePicker from './FilePicker';\r\n\r\n    export default {\r\n        name      : 'asset',\r\n        components: {FilePicker},\r\n        data() {\r\n            return {\r\n                file   : null,\r\n                preview: null\r\n            }\r\n        },\r\n        props     : {\r\n            asset       : {\r\n                type    : String,\r\n                default : null\r\n            },\r\n            assetType   : {\r\n                type    : String,\r\n                default : null\r\n            },\r\n            title       : {\r\n                type    : String\r\n            },\r\n            height      : {\r\n                type    : Number,\r\n                default : 200\r\n            }\r\n            },\r\n        watch     : {\r\n            file(value) {\r\n                this.$emit('input', this.assetType, value);\r\n            }\r\n        },\r\n        methods   : {\r\n            setPreview(value) {\r\n                this.preview = value;\r\n            },\r\n            showOriginal() {\r\n                window.open(this.asset, '_blank');\r\n            },\r\n            deleteFromServer() {\r\n                this.preview = null;\r\n                this.$emit('delete', this.assetType);\r\n            },\r\n            removeFile() {\r\n                this.preview = null;\r\n                this.$emit('remove', this.assetType);\r\n            }\r\n        }\r\n    };\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped lang=\"scss\">\r\n    .preview {\r\n        cursor: pointer;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components/asset"}]}