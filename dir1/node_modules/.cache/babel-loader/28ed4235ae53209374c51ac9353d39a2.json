/../../lib/util/document';\r\n\r\n    export default {\r\n        data() {\r\n            return {\r\n                dialogVisible   : this.visible,\r\n                tweetId         : this.id,\r\n                tweetShowcard   : this.showcard,\r\n                tweetDarktheme  : this.darktheme\r\n            }\r\n        },\r\n        computed: {\r\n            valid() {\r\n                return this.tweetId && (this.tweetId.length > 0);\r\n            }\r\n        },\r\n        methods: {\r\n            onClosed() {\r\n                this.$emit('close', 'tweet', null);\r\n            },\r\n            onSaved() {\r\n                this.$emit('close', 'tweet', {\r\n                    id          : this.tweetId,\r\n                    showcard    : this.tweetShowcard,\r\n                    darktheme   : this.tweetDarktheme\r\n                });\r\n            }\r\n        },\r\n        watch: {\r\n            visible() {\r\n                this.dialogVisible = this.visible;\r\n            },\r\n            dialogVisible() {\r\n                if (this.dialogVisible) {\r\n                    this.tweetId        = this.id;\r\n                    this.tweetShowcard  = this.showcard;\r\n                    this.tweetDarktheme = this.darktheme;\r\n                } else {\r\n                    this.onClosed();\r\n                }\r\n            }\r\n        },\r\n        props  : {\r\n            visible     : {\r\n                type    : Boolean,\r\n                default : false\r\n            },\r\n            newitem     : {\r\n                type    : Boolean,\r\n                default : false\r\n            },\r\n            id          : {\r\n                type    : String,\r\n                default : null\r\n            },\r\n            showcard    : {\r\n                type    : Boolean,\r\n                default : true\r\n            },\r\n            darktheme   : {\r\n                type    : Boolean,\r\n                default : false